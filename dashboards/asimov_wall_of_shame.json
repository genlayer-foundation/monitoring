          "options": {
            "alias": "Shame?",
            "mode": "reduceRow",
            "reduce": {
              "reducers": [
                "lastNotNull"
              ]
            },
            "replaceAllFields": false,
            "replacements": [
              {
                "condition": "isNull",
                "replacement": "YES (нет телеметрии)",
                "targetField": "Version"
              },
              {
                "condition": "neq v0.4.5",
                "replacement": "YES (старая версия)",
                "targetField": "Version"
              },
              {
                "condition": "eq v0.4.5",
                "replacement": "NO",
                "targetField": "Version"
